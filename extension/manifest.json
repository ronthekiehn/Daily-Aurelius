{
    "manifest_version": 3,
    "name": "Daily Aurelius",
    "description": "Shows a new Marcus Aurelius quote every day",
    "version": "1.1",
    "permissions": [
    "storage"
    ],
    "icons": {
      "16": "images/marcuslogo16.png",
      "32": "images/marcuslogo32.png",
      "128": "images/marcuslogo128.png",
      "600": "images/marcuslogo.png"
    },
    "action": {
      "default_icon": "images/marcuslogo128.png",
      "default_popup": "popup.html"
    }
  }
  